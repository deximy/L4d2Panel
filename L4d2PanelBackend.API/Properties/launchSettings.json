{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "DockerWithHttps": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true,
      "sslPort": 5001
    },
    "DockerWithHttp": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": false,
      "httpPort": 5000
    }
  }
}